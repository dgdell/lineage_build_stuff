From ac458e602abaf16c0ee19e70c743383b8a0b575a Mon Sep 17 00:00:00 2001
From: Christopher Wiley <wiley@google.com>
Date: Tue, 19 Apr 2016 12:39:51 -0700
Subject: [PATCH 7/8] msm8974: libexternal should depend on libmedia
MIME-Version: 1.0
Content-Type: text/plain; charset=UTF-8
Content-Transfer-Encoding: 8bit

This library's source code includes libmedia headers.

Bug: 27804373
Change-Id: I6650333a0f68d3a3667f473a6df7a48fa1b1ffa1
Test: Compiles.
Signed-off-by: D. Andrei Măceș <andrei@unlegacy-android.org>
---
 msm8974/libexternal/Android.mk | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/msm8974/libexternal/Android.mk b/msm8974/libexternal/Android.mk
index a82037a65..228ebfc1e 100644
--- a/msm8974/libexternal/Android.mk
+++ b/msm8974/libexternal/Android.mk
@@ -6,7 +6,7 @@ LOCAL_MODULE                  := libexternal
 LOCAL_MODULE_PATH             := $(TARGET_OUT_SHARED_LIBRARIES)
 LOCAL_MODULE_TAGS             := optional
 LOCAL_C_INCLUDES              := $(common_includes) $(kernel_includes)
-LOCAL_SHARED_LIBRARIES        := $(common_libs)
+LOCAL_SHARED_LIBRARIES        := $(common_libs) libmedia
 LOCAL_CFLAGS                  := $(common_flags) -DLOG_TAG=\"qdexternal\"
 LOCAL_ADDITIONAL_DEPENDENCIES := $(common_deps)
 LOCAL_SRC_FILES               := external.cpp
