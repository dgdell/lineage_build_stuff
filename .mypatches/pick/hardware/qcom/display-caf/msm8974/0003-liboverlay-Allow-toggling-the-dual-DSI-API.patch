From ce02fff26bd5c37f200f9dec8a5e76b2f636deb8 Mon Sep 17 00:00:00 2001
From: myfluxi <linflux@arcor.de>
Date: Tue, 29 Sep 2015 20:54:16 +0200
Subject: [PATCH 3/3] liboverlay: Allow toggling the dual DSI API
MIME-Version: 1.0
Content-Type: text/plain; charset=UTF-8
Content-Transfer-Encoding: 8bit

Change-Id: Ib346a1d01218b30aa9c5a1c817bb585fa792829f
Recovered from an ancient CM13 hammerheadcaf change.
Signed-off-by: D. Andrei Măceș <andrei@unlegacy-android.org>
---
 liboverlay/Android.mk | 3 +++
 1 file changed, 3 insertions(+)

diff --git a/liboverlay/Android.mk b/liboverlay/Android.mk
index 8f01b1928..8abe33812 100644
--- a/liboverlay/Android.mk
+++ b/liboverlay/Android.mk
@@ -12,6 +12,9 @@ LOCAL_CFLAGS                  := $(common_flags) -DLOG_TAG=\"qdoverlay\"
 ifeq ($(TARGET_USES_QSEED_SCALAR),true)
     LOCAL_CFLAGS += -DUSES_QSEED_SCALAR
 endif
+ifeq ($(TARGET_USES_DUAL_DSI_API),true)
+    LOCAL_CFLAGS += -DDUAL_DSI
+endif
 LOCAL_ADDITIONAL_DEPENDENCIES := $(common_deps)
 LOCAL_SRC_FILES := \
       overlay.cpp \
-- 
