Subject: [PATCH] Revert "Look for libnqp61-jcop-kit.so in the vendor"

This reverts commit ffaea6aa4d91912f3b80dddd40a03bcec5916f6d.

Change-Id: Ibda2876146ccc754c7d0d36193aef22a824b0484
---
 nci/jni/JcopManager.cpp | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/nci/jni/JcopManager.cpp b/nci/jni/JcopManager.cpp
index a57a74c1..a1421408 100644
--- a/nci/jni/JcopManager.cpp
+++ b/nci/jni/JcopManager.cpp
@@ -80,9 +80,9 @@ tNFA_STATUS JcopManager::JcopInitialize ()
 {
     tNFA_STATUS wStatus = NFA_STATUS_OK;
     //Getting pointer to JCOP module
-    Pgpx_Jcop_handle = dlopen("vendor/lib64/libnqp61-jcop-kit.so",RTLD_NOW);
+    Pgpx_Jcop_handle = dlopen("system/lib64/libnqp61-jcop-kit.so",RTLD_NOW);
     if(Pgpx_Jcop_handle == NULL){
-        ALOGE("%s: Error : opening (vendor/lib64/libnqp61-jcop-kit.so) !!", __func__);
+        ALOGE("%s: Error : opening (system/lib64/libnqp61-jcop-kit.so) !!", __func__);
         return NFA_STATUS_FAILED;
     }
     // Getting pointer to ALA_Init function
@@ -161,7 +161,7 @@ tNFA_STATUS JcopManager::JcopDeInitialize ()
 {
     if(Pgpx_Jcop_handle != NULL){
         if((dlclose(Pgpx_Jcop_handle)) != DL_STATUS_OK){
-            ALOGE("%s: Error : closing (vendor/lib64/libnqp61-jcop-kit.so) !!", __func__);
+            ALOGE("%s: Error : closing (system/lib64/libnqp61-jcop-kit.so) !!", __func__);
             return NFA_STATUS_FAILED;
         }
     }else{
