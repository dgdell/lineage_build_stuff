From 0473dde2240c51d1c607413e676b40c0c0af82c7 Mon Sep 17 00:00:00 2001
From: Simon Shields <simon@lineageos.org>
Date: Thu, 25 Jan 2018 21:28:33 +1100
Subject: [PATCH 7/9] lineageremote: try private remote before giving up

Change-Id: I9b53ef9e70d380cbd4108b6ba9f47d3a5d4c404a
---
 build/envsetup.sh | 4 ++++
 1 file changed, 4 insertions(+)

diff --git a/build/envsetup.sh b/build/envsetup.sh
index d10c54b..31a102c 100644
--- a/build/envsetup.sh
+++ b/build/envsetup.sh
@@ -267,6 +267,10 @@ function lineageremote()
     local GERRIT_REMOTE=$(git config --get remote.github.projectname)
     if [ -z "$GERRIT_REMOTE" ]
     then
+        local GERRIT_REMOTE=$(git config --get remote.private.projectname)
+    fi
+    if [ -z "GERRIT_REMOTE" ]
+    then
         local GERRIT_REMOTE=$(git config --get remote.aosp.projectname | sed s#platform/#android/#g | sed s#/#_#g)
         local PFX="LineageOS/"
     fi
