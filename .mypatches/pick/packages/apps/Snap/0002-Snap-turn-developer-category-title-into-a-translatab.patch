From 81c113b0b0b41e99a3c4d9c831cdc3ddcc6cec05 Mon Sep 17 00:00:00 2001
From: Andreas Blaesius <andi@unlegacy-android.org>
Date: Fri, 8 Jun 2018 13:09:23 +0200
Subject: [PATCH 2/3] Snap: turn developer category title into a translatable
 string

Change-Id: Id543286b1b823adccbab0fabdea1c8df8703f2ad
---
 res/values/cm_strings.xml            | 1 +
 res/xml/setting_menu_preferences.xml | 2 +-
 2 files changed, 2 insertions(+), 1 deletion(-)

diff --git a/res/values/cm_strings.xml b/res/values/cm_strings.xml
index 1f7992239..bbdd644aa 100644
--- a/res/values/cm_strings.xml
+++ b/res/values/cm_strings.xml
@@ -147,5 +147,6 @@
     <!-- Advanced options -->
     <string name="developer_menu_enabled">You have enabled advanced camera options</string>
     <string name="developer_menu_disabled">You have disabled advanced camera options</string>
+    <string name="pref_camera2_preferences_category_developer">Developer options</string>
 
 </resources>
diff --git a/res/xml/setting_menu_preferences.xml b/res/xml/setting_menu_preferences.xml
index 589c02cdf..edc8bba88 100644
--- a/res/xml/setting_menu_preferences.xml
+++ b/res/xml/setting_menu_preferences.xml
@@ -267,7 +267,7 @@
     <PreferenceCategory
         android:key="developer"
         android:layout="@layout/preferences_category"
-        android:title="DEVELOPER OPTIONS">
+        android:title="@string/pref_camera2_preferences_category_developer">
 
         <ListPreference
             android:defaultValue="@string/pref_camera2_instant_aec_default"
