Signed-off-by: Joey <joey@lineageos.org>
---
 AndroidManifest.xml                     | 2 +-
 res/values/cm_strings.xml               | 3 +++
 res/xml/screen_lock_settings.xml        | 2 +-
 res/xml/security_dashboard_settings.xml | 4 ++--
 4 files changed, 7 insertions(+), 4 deletions(-)

diff --git a/AndroidManifest.xml b/AndroidManifest.xml
index 312c3754e2..fd2043b4df 100644
--- a/AndroidManifest.xml
+++ b/AndroidManifest.xml
@@ -1308,7 +1308,7 @@
 
         <activity
             android:name=".Settings$SecurityDashboardActivity"
-            android:label="@string/security_settings_title"
+            android:label="@string/security_privacy_settings_title"
             android:icon="@drawable/ic_homepage_security"
             android:configChanges="orientation|keyboardHidden|screenSize"
             android:taskAffinity="com.android.settings"
diff --git a/res/values/cm_strings.xml b/res/values/cm_strings.xml
index 3eee2ef794..3dfc5ac883 100644
--- a/res/values/cm_strings.xml
+++ b/res/values/cm_strings.xml
@@ -82,6 +82,9 @@
     <string name="proximity_wake_title">Prevent accidental wake-up</string>
     <string name="proximity_wake_summary">Check the proximity sensor prior to waking up screen</string>
 
+    <!-- Security & privacy settings -->
+    <string name="security_privacy_settings_title">Security &amp; privacy</string>
+
     <!-- Setting checkbox title for root access -->
     <string name="root_access">Root access</string>
     <string name="root_access_warning_title">Allow root access?</string>
diff --git a/res/xml/screen_lock_settings.xml b/res/xml/screen_lock_settings.xml
index 5e723ff7bc..d0ef4ddc8d 100644
--- a/res/xml/screen_lock_settings.xml
+++ b/res/xml/screen_lock_settings.xml
@@ -18,7 +18,7 @@
 <PreferenceScreen
     xmlns:android="http://schemas.android.com/apk/res/android"
     android:key="security_settings_password_sub_screen"
-    android:title="@string/security_settings_title">
+    android:title="@string/security_privacy_settings_title">
 
     <!-- available in pattern -->
     <SwitchPreference
diff --git a/res/xml/security_dashboard_settings.xml b/res/xml/security_dashboard_settings.xml
index 7519cefbad..bf8530ec30 100644
--- a/res/xml/security_dashboard_settings.xml
+++ b/res/xml/security_dashboard_settings.xml
@@ -19,7 +19,7 @@
     xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:settings="http://schemas.android.com/apk/res-auto"
     android:key="security_dashboard_page"
-    android:title="@string/security_settings_title"
+    android:title="@string/security_privacy_settings_title"
     settings:initialExpandedChildrenCount="9">
 
     <!-- security_settings_status.xml -->
@@ -161,4 +161,4 @@
         android:summary="@string/summary_placeholder"
         android:fragment="com.android.settings.security.ScreenPinningSettings" />
 
-</PreferenceScreen>
\ No newline at end of file
+</PreferenceScreen>
