From 1c365cceb60ec47509671a1de7126d74286503e9 Mon Sep 17 00:00:00 2001
From: LuK1337 <priv.luk@gmail.com>
Date: Thu, 28 Dec 2017 15:58:02 +0100
Subject: [PATCH 5/8] lineage-sdk: Add kill app back button configs and strings

Change-Id: I867de293fc6e160a86a64d96035704d82b270f4e
---
 lineage/res/res/values/config.xml  | 4 ++++
 lineage/res/res/values/strings.xml | 3 +++
 lineage/res/res/values/symbols.xml | 4 ++++
 3 files changed, 11 insertions(+)

diff --git a/lineage/res/res/values/config.xml b/lineage/res/res/values/config.xml
index b12a8c7..334cdff 100644
--- a/lineage/res/res/values/config.xml
+++ b/lineage/res/res/values/config.xml
@@ -232,4 +232,8 @@
          sdk/src/java/org/lineageos/internal/util/DeviceKeysConstants.java.
     -->
     <integer name="config_longPressOnAppSwitchBehavior">9</integer>
+
+    <!-- Timeout in MS for how long you have to long-press the back key to
+         kill the foreground app. -->
+    <integer name="config_backKillTimeout">2000</integer>
 </resources>
diff --git a/lineage/res/res/values/strings.xml b/lineage/res/res/values/strings.xml
index f93db96..4242a83 100644
--- a/lineage/res/res/values/strings.xml
+++ b/lineage/res/res/values/strings.xml
@@ -205,4 +205,7 @@
 
     <!-- General purpose use "unknown" string -->
     <string name="unknown">Unknown</string>
+
+    <!-- Long-press back kill application -->
+    <string name="app_killed_message">Application killed</string>
 </resources>
diff --git a/lineage/res/res/values/symbols.xml b/lineage/res/res/values/symbols.xml
index e65fa9d..fa672db 100644
--- a/lineage/res/res/values/symbols.xml
+++ b/lineage/res/res/values/symbols.xml
@@ -144,4 +144,8 @@
     <!-- Last app switch animations -->
     <java-symbol type="anim" name="last_app_in" />
     <java-symbol type="anim" name="last_app_out" />
+
+    <!-- Back kill -->
+    <java-symbol type="string" name="app_killed_message" />
+    <java-symbol type="integer" name="config_backKillTimeout" />
 </resources>
-- 
2.7.4

