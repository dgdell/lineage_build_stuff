From: Demon Singur <demonsingur@gmail.com>
Date: Tue, 5 Jun 2018 14:35:29 +0000
Subject: [PATCH 06/10] sdk: add aqua accent

Change-Id: If2fab6acaf5756639f05f4a4d8161af227722891
---
 lineage/res/res/values/arrays.xml  | 1 +
 lineage/res/res/values/strings.xml | 1 +
 lineage/res/res/values/symbols.xml | 1 +
 3 files changed, 3 insertions(+)

diff --git a/lineage/res/res/values/arrays.xml b/lineage/res/res/values/arrays.xml
index 9a5e58c..1c3d086 100644
--- a/lineage/res/res/values/arrays.xml
+++ b/lineage/res/res/values/arrays.xml
@@ -118,6 +118,7 @@
 
     <!-- Trusted accents package names -->
     <string-array name="trusted_accent_packages" translatable="false">
+        <item>org.lineageos.overlay.accent.aqua</item>
         <item>org.lineageos.overlay.accent.red</item>
         <item>org.lineageos.overlay.accent.pink</item>
         <item>org.lineageos.overlay.accent.purple</item>
diff --git a/lineage/res/res/values/strings.xml b/lineage/res/res/values/strings.xml
index f44ed0c..235b559 100644
--- a/lineage/res/res/values/strings.xml
+++ b/lineage/res/res/values/strings.xml
@@ -166,6 +166,7 @@
     <string name="megabytespersecond_short">MB/s</string>
 
     <!-- Accent colors -->
+    <string name="accent_aqua">Aqua</string>
     <string name="accent_black">Carbon</string>
     <string name="accent_blue">Blueberry</string>
     <string name="accent_brown">Cocoa</string>
diff --git a/lineage/res/res/values/symbols.xml b/lineage/res/res/values/symbols.xml
index 97cda48..758de3b 100644
--- a/lineage/res/res/values/symbols.xml
+++ b/lineage/res/res/values/symbols.xml
@@ -150,6 +150,7 @@
 
     <!-- Accent colors -->
     <java-symbol type="array" name="trusted_accent_packages" />
+    <java-symbol type="string" name="accent_aqua" />
     <java-symbol type="string" name="accent_black" />
     <java-symbol type="string" name="accent_blue" />
     <java-symbol type="string" name="accent_brown" />
