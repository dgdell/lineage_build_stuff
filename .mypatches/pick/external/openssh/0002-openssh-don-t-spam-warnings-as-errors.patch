From 70ad53f1dad86fdbe4d0127d9d48c6a6888a271c Mon Sep 17 00:00:00 2001
From: Rashed Abdel-Tawab <rashed@linux.com>
Date: Sun, 12 Aug 2018 19:08:07 -0700
Subject: [PATCH 2/2] openssh: don't spam warnings as errors

Change-Id: I88dbf17aa4401f2babc20806f1ac8a9f8ff1b842
---
 Android.mk | 5 ++++-
 1 file changed, 4 insertions(+), 1 deletion(-)

diff --git a/Android.mk b/Android.mk
index 2d104b7b..d5f312bb 100644
--- a/Android.mk
+++ b/Android.mk
@@ -125,7 +125,7 @@ LOCAL_SHARED_LIBRARIES += libssl libcrypto libdl libz
 
 LOCAL_MODULE := libssh
 
-LOCAL_CFLAGS += -O3 -Wno-unused-parameter -Wno-macro-redefined
+LOCAL_CFLAGS += -O3 -Wno-unused-parameter -Wno-macro-redefined -Wno-pointer-sign
 
 LOCAL_CFLAGS += -DGCE_PLATFORM_SDK_VERSION=$(PLATFORM_SDK_VERSION)
 ifneq ($(filter gce_x86 calypso, $(TARGET_DEVICE)),)
@@ -136,6 +136,8 @@ ifneq (,$(SSHDIR))
 LOCAL_CFLAGS += -DSSHDIR=\"$(SSHDIR)\"
 endif
 
+LOCAL_CFLAGS += -Wno-sign-compare
+
 include $(BUILD_SHARED_LIBRARY)
 
 ###################### ssh ######################
@@ -264,6 +266,7 @@ LOCAL_SRC_FILES := \
 LOCAL_MODULE := sshd
 
 LOCAL_CFLAGS += -Wno-unused-parameter -Wno-macro-redefined
+LOCAL_CFLAGS += -Wno-error -Wno-unused-parameter -Wno-unused-const-variable
 
 LOCAL_C_INCLUDES := \
     external/zlib \
-- 
2.17.1

