From e6a4d75d822945a5a8133618acc51ed9b1e05533 Mon Sep 17 00:00:00 2001
From: Rashed Abdel-Tawab <rashed@linux.com>
Date: Sun, 12 Aug 2018 19:08:05 -0700
Subject: [PATCH] unrar: don't spam warnings as errors

Change-Id: I477bb5784df3565a83db4db064ab02c5737447a7
---
 Android.mk | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/Android.mk b/Android.mk
index d6cb242..d9d60ac 100644
--- a/Android.mk
+++ b/Android.mk
@@ -16,6 +16,8 @@ LOCAL_C_INCLUDES := src
 LOCAL_CFLAGS := -O2 -Wall -DUNRAR -D__BIONIC__ \
                 -D_LARGEFILE_SOURCE -DNOVOLUME -DRAR_NOCRYPT
 
+LOCAL_CFLAGS += -Wno-error -Wno-unused-variable -Wno-unused-parameter -Wno-sign-compare -Wno-logical-op-parentheses -Wno-dangling-else
+
 LOCAL_ARM_MODE := arm
 
 LOCAL_MODULE := unrar
-- 
2.17.1

