Subject: [PATCH 051/214] Bold headers fix for Windows browsers

---
 style.scss | 5 ++---
 1 file changed, 2 insertions(+), 3 deletions(-)

diff --git a/style.scss b/style.scss
index 6bcc91a..71fc36c 100755
--- a/style.scss
+++ b/style.scss
@@ -33,7 +33,7 @@ body {
 h1, h2, h3, h4, h5, h6 {
   font-family: $helveticaNeue;
   color: $darkerGray;
-  font-weight: 600;
+  font-weight: bold;
   
   line-height: 1.7;
   margin: 1em 0 15px;
@@ -45,7 +45,6 @@ h1, h2, h3, h4, h5, h6 {
 }
   
 h1 {  
-  font-weight: 500;
   font-size: 32px;
   a {
     color: inherit;
@@ -105,7 +104,7 @@ em, i {
 }
 
 strong, b {
-  font-weight: 600;
+  font-weight: bold;
 }
 
 img {
