From 1de08654fabc03d1daf5d6c94f03e7db1d4cc62c Mon Sep 17 00:00:00 2001
From: nx111 <gd.zhangdz@gmail.com>
Date: Wed, 29 Aug 2018 23:22:15 +0800
Subject: [PATCH 3/3] openssh: don't spam warnings as errors more.

Change-Id: I55d89ebdda52b214733befc25a41d2651930b654
---
 Android.mk | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/Android.mk b/Android.mk
index abbdaeb6..6de4f298 100644
--- a/Android.mk
+++ b/Android.mk
@@ -125,7 +125,7 @@ LOCAL_SHARED_LIBRARIES += libssl libcrypto libdl libz
 
 LOCAL_MODULE := libssh
 
-LOCAL_CFLAGS += -O3 -Wno-unused-parameter -Wno-macro-redefined -Wno-pointer-sign
+LOCAL_CFLAGS += -O3 -Wno-unused-parameter -Wno-macro-redefined -Wno-pointer-sign -Wno-sign-compare
 
 LOCAL_CFLAGS += -DGCE_PLATFORM_SDK_VERSION=$(PLATFORM_SDK_VERSION)
 ifneq ($(filter gce_x86 calypso, $(TARGET_DEVICE)),)
@@ -266,7 +266,7 @@ LOCAL_SRC_FILES := \
 LOCAL_MODULE := sshd
 
 LOCAL_CFLAGS += -Wno-unused-parameter -Wno-macro-redefined
-LOCAL_CFLAGS += -Wno-error -Wno-unused-parameter -Wno-unused-const-variable
+LOCAL_CFLAGS += -Wno-error -Wno-unused-parameter -Wno-unused-const-variable -Wno-pointer-sign
 
 LOCAL_C_INCLUDES := \
     external/zlib \
-- 
2.17.1

