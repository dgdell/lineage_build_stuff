From: nx111 <gd.zhangdz@gmail.com>
Date: Sat, 29 Sep 2018 02:25:15 +0800
Subject: [PATCH 15/16] msm8974-common: Switch to 1.1 power HAL

Change-Id: I3691bb017c54957d493123107e9e2347a96b5759
---
 BoardConfigCommon.mk | 2 +-
 manifest.xml         | 2 +-
 msm8974.mk           | 2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/BoardConfigCommon.mk b/BoardConfigCommon.mk
index a295db6..6ef0a37 100644
--- a/BoardConfigCommon.mk
+++ b/BoardConfigCommon.mk
@@ -90,7 +90,7 @@ TARGET_RECOVERY_DEVICE_MODULES := libinit_msm8974
 
 # Power
 TARGET_HAS_LEGACY_POWER_STATS := true
-TARGET_HAS_NO_WIFI_STATS := true
+TARGET_HAS_NO_WLAN_STATS := true
 TARGET_USES_INTERACTION_BOOST := true
 
 # Time services
diff --git a/manifest.xml b/manifest.xml
index 942c8f4..c3af01f 100644
--- a/manifest.xml
+++ b/manifest.xml
@@ -125,7 +125,7 @@
     <hal format="hidl">
         <name>android.hardware.power</name>
         <transport>hwbinder</transport>
-        <version>1.0</version>
+        <version>1.1</version>
         <interface>
             <name>IPower</name>
             <instance>default</instance>
diff --git a/msm8974.mk b/msm8974.mk
index 79a02e5..26865f0 100644
--- a/msm8974.mk
+++ b/msm8974.mk
@@ -160,7 +160,7 @@ PRODUCT_PACKAGES += \
 
 # Power HAL
 PRODUCT_PACKAGES += \
-    android.hardware.power@1.0-service-qti
+    android.hardware.power@1.1-service-qti
 
 # RenderScript HAL
 PRODUCT_PACKAGES += \
